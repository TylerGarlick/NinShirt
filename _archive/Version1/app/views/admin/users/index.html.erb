<h1>Users</h1>
<table>
	<thead>
		<tr>
			<th></th>
			<th>First Name</th>
			<th>Last Name</th>
			<th>User Name</th>
			<th>Email</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
			<tr>
				<td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
				<td><%= user.first_name %></td>
				<td><%= user.last_name %></td>
				<td><%= user.username %></td>
				<td><%= user.email %></td>
				<td><%= link_to 'Delete', admin_user_path(user), :method => 'Delete', :confirm => 'Are you sure you want to delete this user?  This is permanent!' %></td>
			</tr>
		<% end %>
	</tbody>
</table>